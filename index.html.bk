        const searchResultsDropdown = document.getElementById('searchResultsDropdown');
        const mobileSearchResultsDropdown = document.getElementById('mobileSearchResultsDropdown');

        // Function to create and append a blog card
        function createBlogCard(article) {
            const cardLink = document.createElement('a');
            cardLink.href = article.link;
            cardLink.classList.add('block', 'blog-card');
            
            // AdSense Policy Optimization: Ensure descriptions are sufficiently long for content quality
            const truncatedDescription = article.description.length > 150 
                ? article.description.substring(0, 150) + '...' 
                : article.description;

            cardLink.innerHTML = `
                <img src="${article.image || 'https://placehold.co/400x200/1a202c/e2e8f0?text=TechyNoob'}" alt="${article.title}" class="w-full h-48 object-cover rounded-t-xl">
                <div class="blog-card-content">
                    <h3>${article.title}</h3>
                    <p class="text-sm text-gray-300">${truncatedDescription}</p>
                </div>
                <div class="blog-card-footer">
                    <span class="text-xs text-gray-400">Read More</span>
