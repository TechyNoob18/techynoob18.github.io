            text-decoration: none;
            transition: color 0.3s ease;
        }
        .blog-card-footer a:hover {
            color: #3B82F6; /* Blue on hover */
        }
        /* Game card specific styles */
        .game-card {
            background-color: #2d3748; /* Darker card background */
